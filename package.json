{
  "name": "@eaxelgm/vuetify-extra-components",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "author": "Axel Gonzalez (https://github.com/EAxelGM/)",
  "license": "MIT",
  "publishConfig": { "access": "public" },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*",
      "require": "./dist/components/*"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EAxelGM/vuetify-extra-components"
  },
  "bugs": {
    "url": "https://github.com/EAxelGM/vuetify-extra-components/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dist": "node scripts/copy-components.js",
    "deploy": "npm run dist && npm publish",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "local": "nuxt dev --host=0.0.0.0",
    "start": "node scripts/dev.js",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@pinia/nuxt": "^0.11.2",
    "json-as-xlsx": "^2.5.6",
    "moment": "^2.30.1",
    "nuxt": "^3.18.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "sass-embedded": "^1.92.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vuetify": "^3.9.6"
  }
}
